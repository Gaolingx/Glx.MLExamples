{
  "seed": 42,
  "output_dir": "outputs/sd15-lightning",
  "pretrained_model_name_or_path": "runwayml/stable-diffusion-v1-5",
  "clip_model_name_or_path": "CompVis/stable-diffusion-v1-4",
  "vae_model_name_or_path": "stabilityai/sd-vae-ft-mse",
  "unet_config_path": "models/unet/config.json",
  "dataset": {
    "name": "reach-vb/pokemon-blip-captions",
    "split": "train",
    "image_column": "image",
    "caption_column": "text",
    "cache_dir": "./.cache/huggingface",
    "max_train_samples": 833,
    "resolution": 512,
    "center_crop": true,
    "random_flip": true,
    "num_workers": 4
  },
  "training": {
    "batch_size": 2,
    "max_epochs": 1,
    "max_steps": 200,
    "accumulate_grad_batches": 1,
    "precision": "16-mixed",
    "learning_rate": 0.0001,
    "weight_decay": 0.01,
    "adam_beta1": 0.9,
    "adam_beta2": 0.999,
    "adam_epsilon": 1e-08,
    "max_grad_norm": 1.0,
    "lr_scheduler": "cosine",
    "lr_warmup_steps": 100,
    "num_train_timesteps": 1000,
    "prediction_type": "epsilon",
    "gradient_checkpointing": false,
    "enable_xformers_memory_efficient_attention": false,
    "manual_optimization": false
  },
  "validation": {
    "enabled": true,
    "every_n_steps": 100,
    "num_inference_steps": 25,
    "guidance_scale": 7.5,
    "prompts": [
      "portrait photo of a girl, photograph, highly detailed face, depth of field, moody light, golden hour",
      "Astronaut in a jungle, cold color palette, muted colors, detailed, 8k"
    ]
  },
"checkpoint": {
  "dirpath": "outputs/sd15-lightning/checkpoints",
  "save_top_k": 3,
  "monitor": "train/loss_epoch",
  "mode": "min",
  "save_last": true,
  "every_n_train_steps": 100
},
  "logging": {
    "tensorboard_save_dir": "outputs/sd15-lightning/tb_logs",
    "name": "sd15_lightning",
    "log_every_n_steps": 10
  },
  "resume_override": {
    "reset_lr": false,
    "learning_rate": 0.0001,
    "reset_scheduler": false
  },
  "inference": {
    "num_inference_steps": 30,
    "guidance_scale": 7.5,
    "height": 512,
    "width": 512,
    "negative_prompt": "low quality, blurry",
    "output_dir": "outputs/sd15-lightning/inference"
  }
}
